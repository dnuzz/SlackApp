@model SlackApp.Model.BotCooldownEntry

@{
    Layout = "_Layout";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>View</title>
</head>
<body>

    <h4>BotCooldownEntry</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="View">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Botname" class="control-label"></label>
                    <input asp-for="Botname" class="form-control" />
                    <span asp-validation-for="Botname" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Channel" class="control-label"></label>
                    <input asp-for="Channel" class="form-control" />
                    <span asp-validation-for="Channel" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="CooldownEnd" class="control-label"></label>
                    <input asp-for="CooldownEnd" class="form-control" />
                    <span asp-validation-for="CooldownEnd" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="CoolDown" class="control-label"></label>
                    <input asp-for="CoolDown" class="form-control" />
                    <span asp-validation-for="CoolDown" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Save" class="btn btn-default" />
                </div>
            </form>
        </div>
    </div>

    <div>
        <a asp-action="Index">Back to List</a>
    </div>

    @section Scripts {
        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    }
</body>
</html>
